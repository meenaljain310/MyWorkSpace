	//	System.out.println(account.showBalance("9649658180"));
		account.createAccount("Vinayak","9649658180",20000);
	//	System.out.println(account.showBalance("9649658180"));
		account.createAccount("Amit","9649658180",50000);
		System.out.println(account.showBalance("9649658180"));
		System.out.println(account.showBalance("7702508656"));
		
		
		
		
	
	}
